		<testOutputDirectory>${basedir}/target/test-classes</testOutputDirectory>


		<plugins>

			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>gwt-maven-plugin</artifactId>
				<version>1.1</version>
				<executions>
					<execution>
						<phase>process-classes</phase>
						<configuration>
							<output>${gwt.output.directory}/${gwt.module.alias}</output>
							<logLevel>INFO</logLevel>
						</configuration>
						<goals>
							<goal>compile</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<compileTargets>
						<value>org.pentaho.pat.Pat</value>
					</compileTargets>
					<runTarget>pat/Pat.jsp</runTarget>
					<output>${gwt.output.directory}/${gwt.module.alias}</output>
					<logLevel>WARN</logLevel>
					<style>OBF</style>
					<extraJvmArgs>-Xss1024k -Xms64M -Xmx512M -XX:PermSize=256m -XX:MaxPermSize=512m</extraJvmArgs>
					<localWorkers>1</localWorkers>
					<gwtVersion>${gwtVersion}</gwtVersion>
					<debugSuspend>false</debugSuspend>
					<webXml>${basedir}/src/main/webapp/WEB-INF/web.xml</webXml>
					<webResources>
						<resource>
							<directory>${gwt.output.directory}/${gwt.module.alias}</directory>
							<targetPath>${gwt.module.alias}</targetPath>
						</resource>
					</webResources>
					<hostedWebapp>${basedir}/target/gwt</hostedWebapp>
				</configuration>
