<module rename-to='pat'>
<!-- Inherit Mosaic Web Toolkit stuff.                          -->
	<inherits name='org.gwt.mosaic.Mosaic' />
	<inherits name='org.gwt.mosaic.actions.Actions' />
	<inherits name='org.gwt.mosaic.forms.Forms' />
	<!--  <inherits name='org.gwt.mosaic.beansbinding.BeansBinding' />-->

	<inherits name="com.allen_sauer.gwt.dnd.gwt-dnd"/>
	<inherits name="org.gwtwidgets.WidgetLibrary"/>

	
<!--  -->	<inherits name='com.google.gwt.gen2.complexpanel.ComplexPanel' />-->
 <inherits name='com.rednels.ofcgwt.OFCGWT'/>	
	<inherits name='org.gwt.mosaic.theme.standard.StandardResources' />
	<inherits name="org.gwt.mosaic.theme.chrome.ChromeResources" />
	<inherits name="org.gwt.mosaic.theme.dark.DarkResources" />
	<inherits name="org.gwt.mosaic.theme.aegean.AegeanResources" />
	
	<inherits name='org.gwt.mosaic.theme.aegean.Aegean' />
	
	
	<!-- Enable debug ID.                                           -->
	<inherits name="com.google.gwt.user.Debug" />
	<set-property name="gwt.enableDebugId" value="true" />
	
	
<!-- 	<inherits name="com.google.gwt.user.User"/> -->
<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>-->
	<inherits name="com.google.gwt.i18n.I18N"/>
	
	
	<!-- GWT to Spring controllers -->
	<servlet path="/session.rpc" class="org.pentaho.pat.server.servlet.SessionServlet"/>
	<servlet path="/query.rpc" class="org.pentaho.pat.server.servlet.QueryServlet"/>
	<servlet path="/discovery.rpc" class="org.pentaho.pat.server.servlet.DiscoveryServlet"/>
	

	
	   
  <source path='rpc'/>  
	<source path='client' />
  
  <!--<inherits name='org.gwt.mosaic.actions.Actions' />-->
   <!--<stylesheet src='halogen.css' />
  
     
	
	<stylesheet src='widgets.css' />-->
	
	<entry-point class="org.pentaho.pat.client.Pat"/>


	<extend-property name="locale" values="es"/>
  	<extend-property name="locale" values="fr"/>
  	<extend-property name="locale" values="de"/>
  	<extend-property name="locale" values="nl"/>
  	<extend-property name="locale" values="it"/>
	<extend-property name="locale" values="pt"/>
</module>
